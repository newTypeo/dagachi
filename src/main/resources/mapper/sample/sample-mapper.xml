<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="student">
  <select id="findAll" resultType="student">
    select 
    	* 
    from 
    	student
   	order by
   		id desc
  </select>
</mapper>